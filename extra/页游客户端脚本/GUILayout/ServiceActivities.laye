					GUIWndClose(handle)
				end
			end
			
			if not ServiceActivities.InitializeFlag then
				for j = 1, 5 do
					if j == 5 then 
						j =7
					end
					for i = 1, 3 do
						local handle = GetWindow(_Parent, "buttonWin"..j..",edit"..i)
						if handle ~= 0 then
							GUIEditSetBold(handle, true)
						end
					end
				end
				
				local handle = GetWindow(_Parent, "buttonWin1,edit4")
				if handle ~= 0 then
					GUIEditSetBold(handle, true)
				end
			
				for i = 1, 7 do
					local handle = GetWindow(_Parent,"buttonWin6,phasebtn"..i)
