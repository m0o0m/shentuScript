			UI:Lua_SubmitForm("新开服活动","get_image_list", "")
		end	
		
		--//显示 未结束的 活动入口
		function ServiceActivities:ImageUpdata(available,data)
			ServiceActivities.end_tab = data
			local count = 0
			for i = 1, #data do
				if data[i] == 0 then
					count = count + 1
					local handle = GetWindow(ServiceActivities.WndHandle,"button"..i)
					if handle ~= 0 then
						GUIWndSetVisible(handle, true)
						GUI:WndSetPosM(handle, 40, 60+55*count)
					end
					local handle_ = GetWindow(ServiceActivities.WndHandle,"image"..i)
					if available[i] == 1 then
						if handle_ ~= 0 then
							GUIWndSetVisible(handle_, true)
							GUI:WndSetPosM(handle_, 151, 59+55*count)
						end
					else
						if handle_ ~= 0 then
							GUIWndSetVisible(handle_, false)
						end
					end
				end
			end
			
			
		end
		
		function ServiceActivities.OnCloese()
			for i = 1, 5 do 
				local handle = GetWindow(0,"ServiceActivities,buttonWin6,msg"..i)
				if tonumber(handle) ~= 0 then 
					GUI:WndClose(handle)
				end
			end
		end
		
		
		-----------------------------------------------------     特惠礼包
		SpecialGift = {}
