						GUIButtonSetStateTextColor(handle,0,MakeARGB(255, 107, 91, 80))
						GUIButtonSetStateTextColor(handle,1,MakeARGB(255, 107, 91, 80))
						GUIButtonSetStateTextColor(handle,2,MakeARGB(255, 206, 178, 98))
						GUIWndSetTextOffset(handle, 0, 3)
					end
				end
				
				for i = 1, 7 do
					handle = GetWindow(_Parent,"buttonWin"..i..", buttonWin")
					if handle ~= 0 then  
						GUI:WndSetScrollBarImage(handle, 1803800026,1803800030, 1803800044, 1803800035)
					end
				end
				ServiceActivities.InitializeFlag = true
			end
			
			for i = 0 , 7 do 
				if i == 0 then
					ButtonSetIsActivePageBtn(nil,"ServiceActivities,button"..i,true)
					WndSetVisible(nil,"ServiceActivities,buttonWin"..i,true)
				else
					ButtonSetIsActivePageBtn(nil,"ServiceActivities,button"..i,false)
					WndSetVisible(nil,"ServiceActivities,buttonWin"..i,false)
				end	
			end
			local handle = GetWindow(nil,"ServiceActivities,buttonWin0")
