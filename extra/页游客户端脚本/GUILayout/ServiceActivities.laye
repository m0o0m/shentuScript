				WndSetVisible(nil,"ServiceActivities,buttonWin1,buttonWin,wnd3,status3",false)
			end
			
			if rein >= 4 then 
				if tab_level[2][4] == 0 then 
					WndSetVisible(nil,"ServiceActivities,buttonWin1,buttonWin,wnd4,status2",true)
					WndAddEffect(nil,"ServiceActivities,buttonWin1,buttonWin,wnd4,status2", 3020300200, -74, -45, 100, 0)
					WndSetVisible(nil,"ServiceActivities,buttonWin1,buttonWin,wnd4,status1",false)
					WndSetVisible(nil,"ServiceActivities,buttonWin1,buttonWin,wnd4,status3",false)
				else
					WndSetVisible(nil,"ServiceActivities,buttonWin1,buttonWin,wnd4,status3",true)
					WndSetVisible(nil,"ServiceActivities,buttonWin1,buttonWin,wnd4,status1",false)
					WndSetVisible(nil,"ServiceActivities,buttonWin1,buttonWin,wnd4,status2",false)
				end
			else
				WndSetVisible(nil,"ServiceActivities,buttonWin1,buttonWin,wnd4,status1",true)
				WndSetVisible(nil,"ServiceActivities,buttonWin1,buttonWin,wnd4,status2",false)
				WndSetVisible(nil,"ServiceActivities,buttonWin1,buttonWin,wnd4,status3",false)
			end
			
			if rein >= 6 then 
				if tab_level[2][5] == 0 then 
					WndSetVisible(nil,"ServiceActivities,buttonWin1,buttonWin,wnd5,status2",true)
					WndAddEffect(nil,"ServiceActivities,buttonWin1,buttonWin,wnd5,status2", 3020300200, -74, -45, 100, 0)
					WndSetVisible(nil,"ServiceActivities,buttonWin1,buttonWin,wnd5,status1",false)
					WndSetVisible(nil,"ServiceActivities,buttonWin1,buttonWin,wnd5,status3",false)
				else
					WndSetVisible(nil,"ServiceActivities,buttonWin1,buttonWin,wnd5,status3",true)
					WndSetVisible(nil,"ServiceActivities,buttonWin1,buttonWin,wnd5,status1",false)
					WndSetVisible(nil,"ServiceActivities,buttonWin1,buttonWin,wnd5,status2",false)
				end
			else
				WndSetVisible(nil,"ServiceActivities,buttonWin1,buttonWin,wnd5,status1",true)
				WndSetVisible(nil,"ServiceActivities,buttonWin1,buttonWin,wnd5,status2",false)
				WndSetVisible(nil,"ServiceActivities,buttonWin1,buttonWin,wnd5,status3",false)
			end
			
			for i=1,5 do 
				handle =GetWindow(nil,"ServiceActivities,buttonWin1,buttonWin,wnd"..i..",last")
				if handle ~= 0 then
					if tonumber(tab_level[1][i]) == 0 then
						GUIEditSetTextM(handle, "已领完")
						WndSetVisible(nil,"ServiceActivities,buttonWin1,buttonWin,wnd".. i ..",status2",false)
					else 
						GUIEditSetTextM(handle, tab_level[1][i])
					end
				end
				if ServiceActivities.getLevelVisible then 
					for j=1,3 do 
						handle =GetWindow(nil,"ServiceActivities,buttonWin1,buttonWin,wnd"..i..",itemctrl"..j)
						if handle ~= 0 then
							if tonumber(tab_level[5][i][j+3]) ~= 0 then 
								GUIWndSetVisible(handle,true)
								RDItemCtrlSetGUIDataPropByKeyName("","ServiceActivities,buttonWin1,buttonWin,wnd"..i..",itemctrl"..j, tab_level[5][i][j], tab_level[5][i][j+3], true)
							else
								GUIWndSetVisible(handle,false)
							end
						end
					end
				end
			end
			ServiceActivities.getLevelVisible = false
		end 
		
