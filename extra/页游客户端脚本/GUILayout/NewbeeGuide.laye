				elseif 5 == whichWay then					UI:Lua_SubmitForm("NewbeeGuide_s", "NewbeeGuide_LevelJudge", "18")
				elseif 6 == whichWay then					UI:Lua_SubmitForm("NewbeeGuide_s", "NewbeeGuide_LevelJudge", "19")
				end
