				elseif 4 == whichWay then					taskBoss = 10 UI:Lua_SubmitForm("NewbeeGuide_s", "NewbeeGuide_LevelJudge", "9")
				elseif 5 == whichWay then					UI:Lua_SubmitForm("NewbeeGuide_s", "NewbeeGuide_LevelJudge", "14") 
				elseif 6 == whichWay then					UI:Lua_SubmitForm("NewbeeGuide_s", "NewbeeGuide_LevelJudge", "4")
