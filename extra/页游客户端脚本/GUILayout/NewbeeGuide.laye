		function NewbeeGuide.MsgUp(msg)
			local color = MakeARGB(255,255,0,0)
			window_msg_up(handle_current_widget, msg, -220, 10, 30, color, 2)
		end

		function NewbeeGuide.OnClose()
			local fh = WndGetParentM(handle_current_widget)
			if fh ~= 0 then
				for i = 1, 5 do
					local _handle = GetWindow(fh, "msg"..i)
					if _handle ~= 0 then
						GUI:WndClose(_handle)
					end
				end
			end
